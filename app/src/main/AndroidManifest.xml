<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="jp.co.bravesoft.my_gcm_example2">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <permission
        android:name="jp.co.bravesoft.my_gcm_example2.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission
        android:name="jp.co.bravesoft.my_gcm_example2.permission.C2D_MESSAGE" />

    <!-- App receives GCM messages. -->
    <!--
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    -->
    <!-- GCM connects to Google Services. -->
    <!--
    <uses-permission android:name="android.permission.INTERNET" />
    -->
    <!-- Keeps the processor from sleeping when a message is received. -->
    <!--
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    -->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <receiver
            android:name="biz.appvisor.push.android.sdk.AppVisorPushBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="jp.bravesoft.appvisor_push_tester" />
            </intent-filter>
        </receiver>

        <service
            android:name="biz.appvisor.push.android.sdk.AppVisorPushIntentService"
            android:exported="false" />
        <service
            android:name="biz.appvisor.push.android.sdk.NotificationStartService"
            android:exported="false" >
        </service>
        <service
            android:name="biz.appvisor.push.android.sdk.RichPushIntentService"
            android:exported="false" >
        </service>
        <activity
            android:name="biz.appvisor.push.android.sdk.RichPushDialogActivity"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" >
        </activity>

        <!-- [START firebase_service] -->
        <service
            android:name="biz.appvisor.push.android.sdk.AppVisorPushFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
        <!-- [END firebase_service] -->
        <!-- [START firebase_iid_service] -->
        <service
            android:name="biz.appvisor.push.android.sdk.AppVisorPushFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
        <!-- [END firebase_iid_service] -->


    </application>

</manifest>